#include <stdio.h>
#include <stdlib.h>
#include "include/const.h"


// Funcio inclosa al arxiu file_parser.c
int fparser(char *str);

// Funcio que retorna un llistat amb strings que representen
// paths a arxius
int flist(char *str); // file_list.c

/**
 *
 * Main function
 *
 */
 
int main(int argc, char **argv){
    STR_ARRAY paraules;
    // comprobem si hi ha parametre d'entrada
    if (argc != 2){
        printf("Exemple d'us: %s fitxer.cfg\n", argv[0]);
        exit(1);
    }

    // cridem a la funcio del parser
    paraules = flist(argv[1]);

    return 0;
}
